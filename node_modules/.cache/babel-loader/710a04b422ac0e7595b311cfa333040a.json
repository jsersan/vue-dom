A,WAAK,QAAL,GAAgB,IAAhB;AACA,WAAK,UAAL,GAAkB,KAAlB,CAfe,CAiBf;;AACA,WAAK,OAAL,GAAc;AACV,QAAA,MAAM,EAAE,EADE;AAEV,QAAA,QAAQ,EAAE,EAFA;AAGV,QAAA,KAAK,EAAE;AAHG,OAAd;AAKH,KAxBI;AAyBL,IAAA,WAzBK,yBAyBS;AACV,WAAK,QAAL,GAAgB,KAAhB;AACA,WAAK,KAAL,GAAa,KAAb;AACJ;AA5BK,GAdE;AA4CX,EAAA,QAAQ,EAAE;AACN,IAAA,cADM,4BACW;AACb,aAAO,KAAK,OAAL,CAAa,MAAb,CAAoB,MAApB,GAA6B,CAApC;AACH,KAHK;AAIN,IAAA,gBAJM,8BAIa;AACf,aAAO,KAAK,OAAL,CAAa,QAAb,CAAsB,MAAtB,GAA+B,CAAtC;AACH,KANK;AAON,IAAA,aAPM,2BAOU;AACZ,aAAO,KAAK,OAAL,CAAa,KAAb,CAAmB,MAAnB,GAA4B,CAAnC;AACH;AATK;AA5CC,CAAf","sourcesContent":["<template>\n<div id=\"formulario-persona\">\n    <form @submit.prevent=\"enviarFormulario\">\n        <div class=\"container\">\n            <div class=\"row\">\n                <div class=\"col-md-4\">\n                    <div class=\"form-group\">\n                        <label>Nombre</label>\n                        <input\n                            ref=\"nombre\"\n                            v-model=\"persona.nombre\"\n                            type=\"text\"\n                            class=\"form-control\"\n                            :class=\"{ 'is-invalid': procesando && nombreInvalido }\"\n                            @focus=\"resetEstado\"\n                            @keypress=\"resetEstado\"\n                        />\n                    </div>\n                </div>\n                <div class=\"col-md-4\">\n                    <div class=\"form-group\">\n                        <label>Apellido</label>\n                        <input\n                            v-model=\"persona.apellido\" \n                            type=\"text\"\n                            class=\"form-control\"\n                            :class=\"{ 'is-invalid': procesando && apellidoInvalido }\"\n                            @focus=\"resetEstado\"\n                        />\n                    </div>\n                </div>\n                <div class=\"col-md-4\">\n                    <div class=\"form-group\">\n                        <label>Email</label>\n                        <input\n                            v-model=\"persona.email\"\n                            type=\"email\"\n                            :class=\"{ 'is-invalid': procesando && emailInvalido }\"\n                            class=\"form-control\"\n                            @focus=\"resetEstado\"\n                        />\n                    </div>\n                </div>\n            </div>\n            <div class=\"row\">\n                <div class=\"col-md-4\">\n                    <div class=\"form-group\">\n                        <button class=\"btn btn-primary\">AÃ±adir persona</button>\n                    </div>\n                </div>\n            </div>\n        </div>\n        <div class=\"container\">\n            <div class=\"row\">\n                <div class=\"col-md-12\">\n                    <div v-if=\"error && procesando\" class=\"alert alert-danger\" role=\"alert\">\n                        Debes rellenar todos los campos!\n                    </div>\n                    <div v-if=\"correcto\" class=\"alert alert-success\" role=\"alert\">\n                        La persona ha sido agregada correctamente!\n                    </div>\n                </div>\n            </div>\n        </div>\n    </form>\n</div>\n</template>\n\n<script>\nexport default {\n    name: 'formulario-persona',\n    data() {\n      return {\n            procesando: false,\n            correcto: false,\n            error: false,\n            persona: {\n            nombre: '',\n            apellido: '',\n            email: '',\n            }\n      }\n    },\n    methods: {\n        enviarFormulario() {\n            this.procesando = true;\n            this.resetEstado();\n            \n            // Comprobamos la presencia de errores\n            if (this.nombreInvalido || this.apellidoInvalido || this.emailInvalido) {\n                this.error = true;\n                return;\n            }\n\n            this.$emit('add-persona', this.persona);\n            this.$refs.nombre.focus();\n\n            this.error = false;\n            this.correcto = true;\n            this.procesando = false;\n\n            // Restablecemos el valor de la variables\n            this.persona= {\n                nombre: '',\n                apellido: '',\n                email: '',\n            }\n        },\n        resetEstado() {\n            this.correcto = false;\n            this.error = false;\n        }\n    },\n    computed: {\n        nombreInvalido() {\n            return this.persona.nombre.length < 1;\n        },\n        apellidoInvalido() {\n            return this.persona.apellido.length < 1;\n        },\n        emailInvalido() {\n            return this.persona.email.length < 1;\n        },\n    },\n}\n</script>\n\n<style scoped>\nform {\n    margin-bottom: 2rem;\n}\n</style>"],"sourceRoot":""}]}