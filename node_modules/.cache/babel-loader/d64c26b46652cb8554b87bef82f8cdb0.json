CAVF,YAAA,CASM,KATN,EAAA,WAAA,EASM,CARF,WAQE,E,gBAPF,YAAA,CAME,OANF,EAME;;aALW,KAAA,CAAA,OAAA,CAAQ,KAAR,GAAa,M;MAKxB;AAJE,IAAA,IAAI,EAAC,OAIP;AAHG,IAAA,KAAK,EAAA,CAAA;AAAA,oBAAkB,KAAA,CAAA,UAAA,IAAc,QAAA,CAAA;AAAhC,KAAA,EACA,cADA,CAGR;AADG,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA;AAAA,aAAE,QAAA,CAAA,WAAA,OAAA,QAAA,YAAF;AAAA,KAAA;AACR,GANF,E,IAAA,E,EAAA,C,iBACa,KAAA,CAAA,OAAA,CAAQ,K,GAMnB,CATN,CAUE,CAXN,CAYE,CAvCN,CA+CE,EAPF,WAOE,CAhDN,C,EAiDA,YAAA,CAWM,KAXN,EAAA,WAAA,EAWM,CAVF,YAAA,CASM,KATN,EAAA,WAAA,EASM,CARF,YAAA,CAOM,KAPN,EAAA,WAAA,EAOM,CANS,KAAA,CAAA,KAAA,IAAS,KAAA,CAAA,U,kBAApB,YAAA,CAEM,KAFN,EAAA,WAAA,EAAwE,oCAAxE,C,iCAME,EAHS,KAAA,CAAA,Q,kBAAX,YAAA,CAEM,KAFN,EAAA,WAAA,EAA8D,8CAA9D,C,iCAGE,CAPN,CAQE,CATN,CAUE,CAXN,C,CAlDJ,E,EAAA,CA+DE,CAhEN,C","sourcesContent":["<template>\n<div id=\"formulario-persona\">\n    <form @submit.prevent=\"enviarFormulario\">\n        <div class=\"container\">\n            <div class=\"row\">\n                <div class=\"col-md-4\">\n                    <div class=\"form-group\">\n                        <label>Nombre</label>\n                        <input\n                            ref=\"nombre\"\n                            v-model=\"persona.nombre\"\n                            type=\"text\"\n                            class=\"form-control\"\n                            :class=\"{ 'is-invalid': procesando && nombreInvalido }\"\n                            @focus=\"resetEstado\"\n                            @keypress=\"resetEstado\"\n                        />\n                    </div>\n                </div>\n                <div class=\"col-md-4\">\n                    <div class=\"form-group\">\n                        <label>Apellido</label>\n                        <input\n                            v-model=\"persona.apellido\" \n                            type=\"text\"\n                            class=\"form-control\"\n                            :class=\"{ 'is-invalid': procesando && apellidoInvalido }\"\n                            @focus=\"resetEstado\"\n                        />\n                    </div>\n                </div>\n                <div class=\"col-md-4\">\n                    <div class=\"form-group\">\n                        <label>Email</label>\n                        <input\n                            v-model=\"persona.email\"\n                            type=\"email\"\n                            :class=\"{ 'is-invalid': procesando && emailInvalido }\"\n                            class=\"form-control\"\n                            @focus=\"resetEstado\"\n                        />\n                    </div>\n                </div>\n            </div>\n            <div class=\"row\">\n                <div class=\"col-md-4\">\n                    <div class=\"form-group\">\n                        <button class=\"btn btn-primary\">Añadir persona</button>\n                    </div>\n                </div>\n            </div>\n        </div>\n        <div class=\"container\">\n            <div class=\"row\">\n                <div class=\"col-md-12\">\n                    <div v-if=\"error && procesando\" class=\"alert alert-danger\" role=\"alert\">\n                        Debes rellenar todos los campos!\n                    </div>\n                    <div v-if=\"correcto\" class=\"alert alert-success\" role=\"alert\">\n                        La persona ha sido agregada correctamente!\n                    </div>\n                </div>\n            </div>\n        </div>\n    </form>\n</div>\n</template>\n\n<script>\nexport default {\n    name: 'formulario-persona',\n    data() {\n      return {\n            procesando: false,\n            correcto: false,\n            error: false,\n            persona: {\n            nombre: '',\n            apellido: '',\n            email: '',\n            }\n      }\n    },\n    methods: {\n        enviarFormulario() {\n            this.procesando = true;\n            this.resetEstado();\n            \n            // Comprobamos la presencia de errores\n            if (this.nombreInvalido || this.apellidoInvalido || this.emailInvalido) {\n                this.error = true;\n                return;\n            }\n\n            this.$emit('add-persona', this.persona);\n            this.$refs.nombre.focus();\n\n            this.error = false;\n            this.correcto = true;\n            this.procesando = false;\n\n            // Restablecemos el valor de la variables\n            this.persona= {\n                nombre: '',\n                apellido: '',\n                email: '',\n            }\n        },\n        resetEstado() {\n            this.correcto = false;\n            this.error = false;\n        }\n    },\n    computed: {\n        nombreInvalido() {\n            return this.persona.nombre.length < 1;\n        },\n        apellidoInvalido() {\n            return this.persona.apellido.length < 1;\n        },\n        emailInvalido() {\n            return this.persona.email.length < 1;\n        },\n    },\n}\n</script>\n\n<style scoped>\nform {\n    margin-bottom: 2rem;\n}\n</style>"],"sourceRoot":""}]}